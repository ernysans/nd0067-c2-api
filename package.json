{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "jasmine": "jasmine",
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./lib --onSuccess \"node ./lib/server.js\"",
    "build": "npm run clear && npm run lint && npm run compile",
    "build:watch": "npm run clear && npm run lint && npm run compile:watch",
    "clear": "rm -rf ./lib",
    "compile": "npx tsc -p ./tsconfig.json",
    "compile:watch": "npx tsc --watch -p ./tsconfig.json",
    "eslint": "eslint",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "ENV=test && npm run compile && db-migrate --env test up && ENV=test jasmine && db-migrate db:drop test",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.0",
    "pg": "^8.11.3",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-no-explicit-type-exports": "^0.12.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  }
}
